{
  "name": "tsuschedule",
  "version": "1.4.1",
  "description": "'Telegram bot for getting lessons schedule'",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "lint": "eslint '**/*.js'",
    "lint:fix": "eslint '**/*.js' --fix",
    "kill": "kill `sudo netstat -nlp | grep :5000 | grep -oP '(\\d+)(?=/node)'`",
    "switch_to_dev": "wget --spider https://api.telegram.org/bot`sed -En 's/^\\s*\"TELEGRAM_TOKEN\":\\s*\"(.*)\",/\\1/p' config/private.json`/setWebhook\\?url\\= --delete-after && node index.js"
  },
  "engines": {
    "node": "12.2.0"
  },
  "repository": {
    "type": "git",
    "url": "'https://github.com/BlasterAlex/TSUSchedule'"
  },
  "keywords": [
    "bot",
    "telegram",
    "tsu"
  ],
  "author": "BlasterAlex",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "cron": "^1.7.2",
    "crypto": "^1.0.1",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "he": "^1.2.0",
    "moment-timezone": "^0.5.31",
    "mongoose": "^5.10.2",
    "node-fetch": "^2.6.0",
    "node-telegram-bot-api": "^0.30.0",
    "pattern-emitter": "^1.0.0",
    "puppeteer": "^1.20.0",
    "socks5-https-client": "^1.2.1",
    "xlsx": "^0.15.6"
  }
}